hosts    = {{ .DBHOST }}:{{ .DBPORT }}
user     = {{ .DBUSER }}
password = {{ .DBPASS }}
dbname   = {{ .DBNAME }}

option_group = client

query = SELECT goto FROM alias, alias_domain WHERE alias_domain.alias_domain = '%d' AND alias.address = CONCAT('%u', '@', alias_domain.target_domain) AND alias.active = 1 AND alias_domain.active = 1
